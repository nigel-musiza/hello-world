    <fields>
        <fullName>Change_since_Last_Period__c</fullName>
        <description>Percentage change of AUM since last period. If the value is null or 0; 0% is returned</description>
        <externalId>false</externalId>
        <formula>IF(NOT ISBLANK(AUM__c) &amp;&amp; NOT ISBLANK(Last_Period_AUM__c),
    IF(AUM__c &gt;0 &amp;&amp; (Last_Period_AUM__c-1) &gt;0, 
       AUM__c  / Last_Period_AUM__c -1,
       0
      ),
    0
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The % change in this month&apos;s Client AUM c.f. last month.</inlineHelpText>
        <label>Change Since Start of Year (%)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
